{
    "manifest_version": 2,
    "name": "Tracking",
    "version": "1.0.0",
    "description": "Tracking is a small multilingual application to track the time you are working on a project",
    "short_name": "Tracking",
    "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
    "content_scripts": [
      {
        "css": ["background.css"],
        "js": [ "jquery-3.5.1.min.js", "background.js" ],
        "matches": [ "http://*/*", "https://*/*"]
      }
    ],
    "browser_action": {
      "default_title": "",
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png"
      }
    }
}